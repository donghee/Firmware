#!/bin/sh
#
# PX4 FMUv5 specific board MAVLink startup script.
#------------------------------------------------------------------------------

# Start MAVLink on the USB port
mavlink start -d /dev/ttyACM0
mavlink start -d /dev/ttyS2 -b p:SER_TEL2_BAUD -r p:MAV_1_RATE -m custom
dim -S start
param set MAV_PROTO_VER 1
